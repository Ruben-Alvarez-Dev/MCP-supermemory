{
  "mcpServers": {
    "mcp-superserver-local": {
      "command": "node",
      "args": [
        "Docker/mcp-hub/src/mcp-server.js"
      ],
      "cwd": "/Volumes/-Code/_Code/MCP-SUPERSERVER",
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USER": "neo4j",
        "NEO4J_PASSWORD": "change_me_in_production",
        "OLLAMA_HOST": "localhost",
        "OLLAMA_PORT": "11434",
        "OBSIDIAN_VAULT": "/Volumes/-Code/_Code/MCP-SUPERSERVER/data/obsidian",
        "LOG_LEVEL": "debug"
      }
    }
  }
}
